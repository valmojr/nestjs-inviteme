{
  "name": "nestjs-inviteme",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest --watch --coverage",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "prisma:migrate_deploy:raw": "npx prisma migrate deploy",
    "prisma:migrate_dev:raw": "npx prisma migrate dev",
    "prisma:push:raw": "npx prisma db push",
    "prisma:pull:raw": "npx prisma db pull",
    "prisma:generate:raw": "npx prisma generate",
    "prisma:studio:raw": "npx prisma studio",
    "prisma:migrate_deploy:local": "dotenv -e .env.local -- npx prisma migrate deploy",
    "prisma:migrate_dev:local": "dotenv -e .env.local -- npx prisma migrate dev",
    "prisma:push:local": "dotenv -e .env.local -- npx prisma db push",
    "prisma:pull:local": "dotenv -e .env.local -- npx prisma db pull",
    "prisma:generate:local": "dotenv -e .env.local -- npx prisma generate",
    "prisma:studio:local": "dotenv -e .env.local -- npx prisma studio",
    "prisma:migrate_deploy:dev": "dotenv -e .env.dev -- npx prisma migrate deploy",
    "prisma:migrate_dev:dev": "dotenv -e .env.dev -- npx prisma migrate dev",
    "prisma:push:dev": "dotenv -e .env.dev -- npx prisma db push",
    "prisma:pull:dev": "dotenv -e .env.dev -- npx prisma db pull",
    "prisma:generate:dev": "dotenv -e .env.dev -- npx prisma generate",
    "prisma:studio:dev": "dotenv -e .env.dev -- npx prisma studio",
    "prisma:migrate_deploy:prod": "dotenv -e .env.prod -- npx prisma migrate deploy",
    "prisma:migrate_dev:prod": "dotenv -e .env.prod -- npx prisma migrate dev",
    "prisma:push:prod": "dotenv -e .env.prod -- npx prisma db push",
    "prisma:pull:prod": "dotenv -e .env.prod -- npx prisma db pull",
    "prisma:generate:prod": "dotenv -e .env.prod -- npx prisma generate",
    "prisma:studio:prod": "dotenv -e .env.prod -- npx prisma studio"
  },
  "dependencies": {
    "@nestjs/common": "^10.0.0",
    "@nestjs/config": "^3.2.0",
    "@nestjs/core": "^10.0.0",
    "@nestjs/jwt": "^10.2.0",
    "@nestjs/mapped-types": "*",
    "@nestjs/platform-express": "^10.0.0",
    "@prisma/client": "^5.10.2",
    "cookie-parser": "^1.4.6",
    "discord.js": "^14.14.1",
    "dotenv": "^16.4.1",
    "jsonwebtoken": "^9.0.2",
    "necord": "^6.7.0",
    "reflect-metadata": "^0.2.0",
    "rxjs": "^7.8.1"
  },
  "devDependencies": {
    "@necord/schematics": "^1.3.5",
    "@nestjs/cli": "^10.0.0",
    "@nestjs/schematics": "^10.0.0",
    "@nestjs/testing": "^10.0.0",
    "@types/cookie-parser": "^1.4.7",
    "@types/express": "^4.17.17",
    "@types/jest": "^29.5.2",
    "@types/node": "^20.3.1",
    "@types/supertest": "^6.0.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "dotenv-cli": "^7.3.0",
    "eslint": "^8.42.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.0",
    "jest": "^29.5.0",
    "prettier": "^3.0.0",
    "prisma": "^5.10.2",
    "source-map-support": "^0.5.21",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.0",
    "ts-loader": "^9.4.3",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.1.3"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node",
    "coveragePathIgnorePatterns": [
      "/node_modules/",
      "/test/",
      "/dist/",
      "/src/main.ts",
      "/src/app.module.ts",
      "/src/auth/auth.module.ts",
      "/src/event/event.module.ts",
      "/src/user/user.module.ts",
      "/src/group/group.module.ts",
      "/src/house/house.module.ts",
      "/src/role/role.module.ts",
      "/src/user/user.module.ts",
      "/src/discord/discord.module.ts"
    ]
  }
}
